-- marvelUsuarios

create table if not exists marvelusuarios(
	id Serial not null,
	nombre varchar(100) not null,
	identificacion varchar(100) not null,
	correo varchar(100) not null,
	clave varchar(100) not null,
	primary key ("id")
)

